$TTL 86400
@   IN  SOA ns1.so.very.invalid. hostmaster.so.very.invalid. (
        20230123   ; serial
        3600       ; refresh
        1800       ; retry
        604800     ; expire
        86400      ; minimum
)

; Name servers
@       IN  NS  ns1.so.very.invalid.
@       IN  NS  ns2.so.very.invalid.

; A records for name servers
ns1     IN  A   192.168.1.1
ns2     IN  A   192.168.1.2

; A records for fictitious hosts
www     IN  A   192.168.1.10
mail    IN  A   192.168.1.11
ftp     IN  A   192.168.1.12

; CNAME record
webmail IN  CNAME mail.so.very.invalid.

; MX record with a subtle issue (missing dot at the end of the mail.so.very.invalid)
@       IN  MX  10 mail.so.very.invalid
